char data=0;
#include <Servo.h>
Servo myservo1;
Servo myservo2;
Servo myservo3;
Servo myservo4;
Servo myservo5;
Servo myservo6;

void setup() {
 Serial.begin(9600);
  pinMode(5,OUTPUT);
  pinMode(6,OUTPUT);
  pinMode(10,OUTPUT);
  pinMode(11,OUTPUT);
  myservo1.attach(2);
  myservo2.attach(3);
  myservo3.attach(4);
  myservo4.attach(7);
  myservo5.attach(8);
  myservo6.attach(9);
}

void loop() {
   myservo1.write(120);
   myservo2.write(150);
   myservo3.write(180);
   myservo4.write(150);
   myservo5.write(90);
 if(Serial.available()>0)
 {
  data=Serial.read();
  Serial.print(data);
  if(data == 'F') 
      {  
              
        digitalWrite(5, HIGH);
        digitalWrite(6, LOW);
        digitalWrite(10, LOW);
        digitalWrite(11, HIGH);
      }   
       if(data == 'B') 
      {        
         digitalWrite(5, LOW);
        digitalWrite(6, HIGH);
        digitalWrite(10, HIGH);
        digitalWrite(11, LOW);   
         
  }
  if(data == 'L') 
      {        
         digitalWrite(5, LOW);
         digitalWrite(6, HIGH);
         digitalWrite(10, LOW);
         digitalWrite(11, HIGH);    
         
  }
  if(data == 'R') 
      {        
         digitalWrite(5, HIGH);
         digitalWrite(6, LOW);
         digitalWrite(10, HIGH);
         digitalWrite(11, LOW);
        }
  if(data== ' ')
      {        
         digitalWrite(5, LOW);
         digitalWrite(6, LOW);
         digitalWrite(10, LOW);
         digitalWrite(11, LOW);
         }
    if( data== 'Y')
  {
    myservo1.write(120);
  }
    if( data== 'A')
  {
    myservo1.write(20);
  }
  if( data== 'C')
  {
    myservo1.write(50);
  }
  if( data== 'D')
  {
    myservo1.write(120);
  }
  if( data== 'E')
  {
    myservo1.write(150);
  }
  if( data== 'G')
  {
    myservo1.write(180);
  }
  if( data == 'Z')
  {
    myservo2.write(150);
  }
  if( data == 'H')
  {
    myservo2.write(120);
  }
  if( data == 'I')
  {
    myservo2.write(40);
  }
  if( data == 'J')
  {
    myservo2.write(0);
  }
  if( data == 'K')
  {
    myservo3.write(100);
  }
  if( data == 'M')
  {
    myservo3.write(120);
  }
  if( data == 'N')
  {
    myservo3.write(140);
  }
  
  if( data == 'O')
  {
    myservo4.write(150);
  }
  if( data == 'P')
  {
    myservo4.write(120);
  }
  if( data == 'Q')
  {
    myservo4.write(100);
  }
  if( data == 'S')
  {
   myservo5.write(0);
  }
   if( data == 'T')
  {
   myservo5.write(30);
  }
   if( data == 'U')
  {
   myservo5.write(90);
  }
   if( data == 'V')
  {
   myservo5.write(130);
  }
  if( data == 'W')
  {
    myservo6.write(180);
  }
   if( data == 'X')
  {
    myservo6.write(100);
  }
 }
}
